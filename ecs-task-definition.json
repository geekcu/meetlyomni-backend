{
  "family": "meetlyomni-prod-backend",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::120456573741:role/meetlyomni-prod-ecs-execution-role",
  "taskRoleArn":"arn:aws:iam::120456573741:role/meetlyomni-prod-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "meetly-omni-backend",
      "image": "120456573741.dkr.ecr.ap-southeast-2.amazonaws.com/meetly-omni-backend-prod:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        },
        {
          "name": "ASPNETCORE_URLS",
          "value": "http://0.0.0.0:80"
        },
        {
          "name": "Jwt__Issuer",
          "value": "https://api.meetlyomni.com"
        },
        {
          "name": "Jwt__Audience",
          "value": "https://api.meetlyomni.com"
        },
        {
          "name": "NODE_ENV",
          "value": "prod"
        }
      ],
      "secrets": [
        {
          "name": "ConnectionStrings__MeetlyOmniDb",
          "valueFrom": "arn:aws:ssm:ap-southeast-2:120456573741:parameter/meetlyomni/prod/backend/DB_CONNECTION_STRING"
        },
        {
          "name": "JWT_SIGNING_KEY",
          "valueFrom": "arn:aws:ssm:ap-southeast-2:120456573741:parameter/meetlyomni/prod/backend/JWT_SIGNING_KEY"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/meetlyomni/prod/backend",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}